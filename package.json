{
  "name": "tsdown-starter",
  "version": "0.0.0",
  "type": "module",
  "license": "UNLICENSED",
  "main": "./dist/index.js",
  "scripts": {
    "watch:build": "vite build -w",
    "watch:start": "node --env-file=.env --enable-source-maps --watch-preserve-output --watch-path=./dist --watch ./start.mjs",
    "start": "node --env-file=.env --enable-source-maps ./start.mjs",
    "build": "vite build -m production"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "commander": "^14.0.0",
    "i18next": "^25.4.2",
    "kysely": "^0.28.5",
    "pg": "^8.16.3",
    "pino": "^9.9.0",
    "pino-pretty": "^13.1.1",
    "redis": "^5.8.2",
    "sharp": "^0.34.3",
    "telegraf": "^4.16.3",
    "zod": "^4.1.3"
  },
  "devDependencies": {
    "@modyfi/vite-plugin-yaml": "^1.1.1",
    "@types/node": "^22.15.17",
    "@types/pg": "^8.15.5",
    "typescript": "^5.9.2",
    "vite": "npm:rolldown-vite@^7.1.8"
  },
  "pnpm": {
    "overrides": {
      "vite": "npm:rolldown-vite@^7.1.8"
    },
    "onlyBuiltDependencies": [
      "sharp"
    ]
  }
}
